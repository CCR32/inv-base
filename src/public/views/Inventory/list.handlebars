<table class="table table-hover">
    <thead>
        <th>Codigo</th>
        <th>Existencia</th>
        <th>Stock</th>
        <th>Estatus</th>
        <th>Eliminar</th>
    </thead>
    <tbody>        
        {{#if items}}
            {{#each items}}
            <tr>
                <td>{{code}}</td>    
                <td>{{existencia}}</td>
                <td>{{stock}}</td>
                <td>{{estatus}}</td>
                <td><a href="#">Eliminar</a></td>
            </tr>
            {{/each}}
        {{/if}}
    </tbody>
</table>


